<xml>
  <category name="%{BKY_CATBASIC}" colour='#FFCB4A'>
    <block type="basic_start"></block>
    <block type="basic_forever"></block>
    <block type="basic_print">
      <value name="msg">
        <shadow type="text">
          <field name="TEXT">abc</field>
        </shadow>
      </value>
    </block>
    <block type="basic_delay">
      <value name="ms">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
    </block>
  </category>
</xml>